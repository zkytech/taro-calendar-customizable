{
  "name": "taro-calendar-customizable",
  "version": "1.2.1",
  "private": false,
  "description": "可自定义部分样式的taro日历组件",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zkytech/taro-calendar-customizable"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "author": "zkyTech",
  "license": "MIT",
  "dependencies": {
    "@tarojs/components": "1.3.13",
    "@tarojs/taro": "1.3.13",
    "@tarojs/taro-weapp": "^1.3.13"
  },
  "devDependencies": {
    "@tarojs/plugin-babel": "1.3.13",
    "@tarojs/plugin-csso": "1.3.13",
    "@tarojs/plugin-less": "1.3.13",
    "@tarojs/plugin-uglifyjs": "1.3.13",
    "@tarojs/webpack-runner": "1.3.13",
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.16.0",
    "eslint-config-taro": "1.3.13",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-taro": "1.3.13",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "1.3.13",
    "stylelint-taro-rn": "1.3.13",
    "typescript": "^3.0.1"
  },
  "main": "index.ts",
  "types": "index.d.ts",
  "keywords": [
    "calendar",
    "taro",
    "taro-calendar-customizable",
    "customizable"
  ],
  "files": [
    "README.md",
    "index.ts",
    "index.d.ts",
    "src/pages/calendar"
  ]
}
